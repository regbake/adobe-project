<!DOCTYPE html>
<html>
<head>
  <title>front end</title>
</head>
<body>
  <h1>Greetings from  <%= city %></h1>

  <form>
    <input type="text" placeholder="Search for a name" />
    <button>Show data</button>
  </form>

  <table>
    <tr>
      <th>Names!</th>
    </tr>
      <% names.forEach(function(name){ %>
        <tr>
          <td><%= name %></td>
        </tr>
      <% }); %>
  </table>
</body>
</html>
